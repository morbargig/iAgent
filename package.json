{
  "name": "@iagent/workspace",
  "version": "1.0.0",
  "license": "MIT",
  "description": "iAgent AI Chat Application - Nx Workspace",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npx nx serve frontend",
    "dev:backend": "npx nx serve backend",
    "build": "npx nx run-many --target=build --projects=frontend,backend",
    "build:frontend": "npx nx build frontend",
    "build:backend": "npx nx build backend",
    "test": "npx nx run-many --target=test --projects=frontend,backend",
    "test:frontend": "npx nx test frontend",
    "test:backend": "npx nx test backend",
    "lint": "npx nx run-many --target=lint --projects=frontend,backend",
    "lint:frontend": "npx nx lint frontend",
    "lint:backend": "npx nx lint backend",
    "deploy:gh-pages": "bash scripts/deploy-gh-pages.sh",
    "deploy:local": "bash scripts/deploy-gh-pages.sh",
    "upgrade": "npx nx migrate latest && npm install && npx nx migrate --run-migrations",
    "clean": "npx nx reset && rm -rf node_modules && npm install",
    "graph": "npx nx graph",
    "affected": "npx nx affected"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "isguard-ts": "^3.1.0",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.5",
    "reflect-metadata": "^0.2.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
