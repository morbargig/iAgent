    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.39.1",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@nx/eslint": "22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/jest": "22.0.2",
    "@nx/js": "22.0.2",
    "@nx/nest": "22.0.2",
    "@nx/node": "22.0.2",
    "@nx/react": "22.0.2",
    "@nx/rollup": "22.0.2",
    "@nx/vite": "22.0.2",
    "@nx/web": "22.0.2",
    "@nx/webpack": "22.0.2",
    "@nx/workspace": "22.0.2",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^8.1.0",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.8",
    "@swc/core": "~1.15.0",
